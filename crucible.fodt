    </table:table-row>
    <table:table-row>
     <table:table-cell table:style-name="Table2.A3" office:value-type="string">
      <text:p text:style-name="P87">Constitution</text:p>
     </table:table-cell>
     <table:table-cell table:style-name="Table2.A3" office:value-type="string">
      <text:p text:style-name="P87">1<text:span text:style-name="T200">2</text:span></text:p>
