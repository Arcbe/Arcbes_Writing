     </table:table-cell>
     <table:table-cell table:style-name="Table15.A1" office:value-type="string">
      <text:p text:style-name="P133">Akemi Kobayashi</text:p>
     </table:table-cell>
     <table:table-cell table:style-name="Table15.A1" office:value-type="string">
      <text:p text:style-name="P213">Race</text:p>
     </table:table-cell>
     <table:table-cell table:style-name="Table15.D1" office:value-type="string">
      <text:p text:style-name="P220">Outsider</text:p>
     </table:table-cell>
