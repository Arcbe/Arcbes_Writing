     </table:table-cell>
     <table:table-cell table:style-name="Table14.A3" office:value-type="string">
      <text:p text:style-name="P60">1<text:span text:style-name="T120">2</text:span></text:p>
     </table:table-cell>
     <table:table-cell table:style-name="Table14.A3" office:value-type="string">
      <text:p text:style-name="P66">Intelligence</text:p>
     </table:table-cell>
     <table:table-cell table:style-name="Table14.D3" office:value-type="string">
      <text:p text:style-name="P60">1<text:span text:style-name="T120">1</text:span></text:p>
