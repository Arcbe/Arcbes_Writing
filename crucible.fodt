     <table:table-cell table:style-name="Table14.D3" office:value-type="string">
      <text:p text:style-name="P83">1<text:span text:style-name="T138">1</text:span></text:p>
     </table:table-cell>
    </table:table-row>
    <table:table-row>
     <table:table-cell table:style-name="Table14.D3" table:number-columns-spanned="4" office:value-type="string">
      <text:p text:style-name="P83"/>
     </table:table-cell>
