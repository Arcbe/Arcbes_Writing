     <table:table-cell table:style-name="Table8.A1" office:value-type="string">
      <text:p text:style-name="P125">Name</text:p>
     </table:table-cell>
     <table:table-cell table:style-name="Table8.A1" office:value-type="string">
      <text:p text:style-name="P125">Akemi Kobayashi</text:p>
     </table:table-cell>
