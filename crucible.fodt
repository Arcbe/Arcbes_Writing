      <text:p text:style-name="P106">Traits</text:p>
     </table:table-cell>
     <table:table-cell table:style-name="Table14.D1" table:number-columns-spanned="3" office:value-type="string">
      <text:p text:style-name="P99">[Hekate’s Blessing] <text:span text:style-name="T123">[Mana Leak]</text:span></text:p>
