     </table:table-cell>
     <table:table-cell table:style-name="Table4.A1" office:value-type="string">
      <text:p text:style-name="P111">Akemi Kobayashi</text:p>
     </table:table-cell>
     <table:table-cell table:style-name="Table4.A1" office:value-type="string">
      <text:p text:style-name="P193">Race</text:p>
